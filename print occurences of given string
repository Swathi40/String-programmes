#WAP to print count of occurences of empty character#
S   = 'programming'
res = ''
for ch in S:
    if ch not in res:
        res=res+ch
for ch in res:
    print(f'{ch}={S.count(ch)}')
#Output#
p=1
r=2
o=1
g=2
a=1
m=2
i=1
n=1



#WAP to print the occurences of every character#
S = 'engineering'
d = {}
for ch in S:
    if ch not in d:
        d[ch]=1
    else:
        d[ch]=d[ch]+1
for key,val in d.items():
    print(f'{key}={val}')

#Output#
e=3
n=3
g=2
i=2
r=1
